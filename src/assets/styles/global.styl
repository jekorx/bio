html, body
  margin 0
body
  background url("../images/bg.png")
  font-family "Helvetica Neue", Helvetica, "PingFang SC", "Hiragino Sans GB", "Microsoft YaHei", "微软雅黑", Arial, sans-serif
  font-size 14px
  color #4d4d4d
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale
  font-weight 300
  padding 20px 20px 0 20px
  box-sizing border-box
@font-face
  font-family: "iconfont";
  src: url('../fonts/iconfont.eot?t=1518444886601'); /* IE9*/
  src: url('../fonts/iconfont.eot?t=1518444886601#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff;charset=utf-8;base64,') format('woff'),
  url('../fonts/iconfont.ttf?t=1518444886601') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+*/
  url('../fonts/iconfont.svg?t=1518444886601#iconfont') format('svg'); /* iOS 4.1- */
.icon:before
  font-family "iconfont" !important
  font-style normal
  font-size 18px
  margin-right 5px
.icon-work:before
  content "\e61e"
.icon-plane:before
  content "\e606"
.icon-user:before
  content "\ecc8"
.icon-edu:before
  content "\e60b"
.icon-contact:before
  content "\ec81"
.icon-tech:before
  content "\e60c"
.icon-group:before
  content "\e6a0"
.icon-list:before
  content "\e701"
.icon-code:before
  content "\e654"
.icon-terminal:before
  content "\e6a6"
.icon-pencil:before
  content "\e6ef"
h2
  font-size 16px
  border-bottom 1px solid #AAA
  margin 0
  padding-bottom 8px
ul
  list-style none
  padding-left 0
  margin-top 8px
  label
    display inline-block
    width 80px
a
  text-decoration none
  color #42B983
